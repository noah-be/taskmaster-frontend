{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve:test": "node tests/server/test-server.js",
    "test:e2e": "npm-run-all -p serve:test test:e2e:run",
    "test:e2e:run": "vitest run --environment node tests/e2e",
    "test:cleanup": "kill $(lsof -t -i:3000 || echo 0)",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom tests/vue",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:dot": "vitest --silent --reporter dot",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "vite-plugin-vuetify": "^2.0.4",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-istanbul": "^2.1.5",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "@vue/test-utils": "^2.4.6",
    "axe-core": "^4.10.2",
    "c8": "^10.1.2",
    "express": "^4.21.2",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.49.1",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "vite": "^5.4.11",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.8",
    "vue-axe": "^3.1.2",
    "xml": "^1.0.1"
  },
  "prettier": {
    "printWidth": 160,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "endOfLine": "auto"
  }
}
